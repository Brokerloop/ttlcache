{
  "name": "@brokerloop/ttlcache",
  "version": "0.1.2",
  "description": "TTL cache for NodeJS and browsers.",
  "private": true,
  "author": "Alex Soncodi <alex@brokerloop.com>",
  "license": "MIT",
  "keywords": [
    "lru",
    "ttl",
    "cache",
    "expire",
    "expiry",
    "expires",
    "expiring",
    "expiration",
    "nodejs",
    "browser",
    "typescript"
  ],
  "scripts": {
    "clean": "rm -rf ./build ./coverage ./.nyc_output",
    "lint": "tslint -p tsconfig.json",
    "test": "tsc && alsatian \"./build/test/**/*.test.js\"",
    "coverage": "nyc --reporter=lcov npm run test && nyc report",
    "build": "tsc",
    "watch": "tsc --watch",
    "package": "npm run clean && npm run build && ./package.sh",
    "publish": "npm run package && ./publish.sh"
  },
  "devDependencies": {
    "alsatian": "~2.2.1",
    "nyc": "~12.0.2"
  }
}
